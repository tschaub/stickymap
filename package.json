{
  "name": "stickymap",
  "version": "1.3.3",
  "main": "src/index.js",
  "scripts": {
    "pretest": "eslint src test",
    "test": "karma start test/config.js --single-run",
    "test-forever": "karma start test/config.js",
    "serve": "browser-sync start --server build --files build --reload-debounce 1000 --no-open",
    "rebuild": "nodemon --ext js,html --watch src --watch doc --exec make build",
    "start": "npm run rebuild & npm run serve"
  },
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "chai": "^4.1.2",
    "eslint": "^4.18.2",
    "eslint-config-tschaub": "^9.0.0",
    "karma": "^2.0.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^3.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-source-map-support": "^1.1.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.11.0",
    "pixelmatch": "^4.0.2",
    "watchify": "^3.9.0"
  },
  "eslintConfig": {
    "extends": "tschaub"
  }
}
